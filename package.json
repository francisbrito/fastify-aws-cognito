    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "fastify": "^2.6.0",
    "fastify-plugin": "^1.6.0",
